      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning",
        "uSync.Migrations": "Debug"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "d8c0eb20-2d85-4af1-8173-97aba93a5d36",
        "SanitizeTinyMce": true
      },
      "Security": {
        "UsernameIsEmail": false,
        "KeepUserLoggedIn": true,
